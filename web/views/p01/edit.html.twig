									<div class='form-group'>
									    {{ form_label(form.Miniature, 'Miniature') }}
									    {{ form_widget(form.Miniature, { attr: { 'class': 'form-control' }}) }}
									</div>

									<div class='form-group'>
									    {{ form_label(form.PushPull, 'PushPull') }}
									    {{ form_widget(form.PushPull, { attr: { 'class': 'form-control' }}) }}
									</div>

									<div class='form-group'>
									    {{ form_label(form.Fiber, 'Fiber') }}
									    {{ form_widget(form.Fiber, { attr: { 'class': 'form-control' }}) }}
									</div>

									<div class='form-group'>
									    {{ form_label(form.WaterProof, 'WaterProof') }}
