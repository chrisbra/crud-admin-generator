									</div>

									<div class='form-group'>
									    {{ form_label(form.HighSpeed, 'HighSpeed') }}
									    {{ form_widget(form.HighSpeed, { attr: { 'class': 'form-control' }}) }}
									</div>

									<div class='form-group'>
									    {{ form_label(form.Standard, 'Standard') }}
									    {{ form_widget(form.Standard, { attr: { 'class': 'form-control' }}) }}
									</div>

									<div class='form-group'>
